{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*",
        "ahmed-aliraqi/laravel-media-uploader": "^6.0",
        "astrotomic/laravel-translatable": "^11.9",
        "christiankuri/laravel-favorite": "^1.4",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laracasts/flash": "^3.2",
        "laraeast/laravel-settings": "^1.1",
        "laravel/framework": "^8.12",
        "laravel/sanctum": "^2.8",
        "laravel/slack-notification-channel": "^2.3",
        "laravel/telescope": "^4.5",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.1",
        "maatwebsite/excel": "^3.1",
        "malhal/laravel-geographical": "^1.0",
        "owen-it/laravel-auditing": "^12.0",
        "pusher/pusher-php-server": "^4.1",
        "pusher/pusher-push-notifications": "^1.1",
        "spatie/laravel-backup": "^6.15"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.9",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^0.0.5",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/Helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "app:clear": "php artisan optimize:clear",
        "auto-complete:generate": [
            "@php artisan ide-helper:meta --ansi --quiet",
            "@php artisan ide-helper:generate --ansi --quiet",
            "@php artisan ide-helper:models --nowrite --quiet"
        ]
    }
}
